{% extends 'base.html.twig' %}

{% block title %}RoomDate{% endblock %}

{% block body %}
    <h1>RoomDate</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ room_date.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ room_date.date ? room_date.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ room_date.price }}</td>
            </tr>
            <tr>
                <th>Discount</th>
                <td>{{ room_date.discount }}</td>
            </tr>
            <tr>
                <th>RoomCount</th>
                <td>{{ room_date.roomCount }}</td>
            </tr>
            <tr>
                <th>RoomBooked</th>
                <td>{{ room_date.roomBooked }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('room_date_index') }}">back to list</a>

    <a href="{{ path('room_date_edit', {'id': room_date.id}) }}">edit</a>

    {{ include('room_date/_delete_form.html.twig') }}
{% endblock %}
