<form method="post" action="{{ path('room_date_delete', {'id': room_date.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ room_date.id) }}">
    <button class="btn">Delete</button>
</form>
